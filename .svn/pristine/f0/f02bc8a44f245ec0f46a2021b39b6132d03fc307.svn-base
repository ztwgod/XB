package cn.com.xb.domain.displayWrapper;

import java.io.Serializable;
import java.sql.Timestamp;

import cn.com.xb.domain.base.FeeProof;

/**
 * 交易详情封装
 * @author DiGua
 *
 */
public class TransActionDetailWrapper implements Serializable
{
	private static final long serialVersionUID = 1L;
	
	private String transActionId;	// 交易动作ID
	private String transId;			// 交易信息ID
	private int transActionType;	// 交易动作类型
	private Timestamp actionTime;	// 交易动作时间
	private String actioner;		// 动作触发人
	private String actionerName;	// 动作触发人姓名
	private String actionerMobileNum;	// 动作触发人联系电话
	private String boxActionId;		// 物箱端动作ID
	
	private FeeProof feeProof;		// 付费凭证信息
	
	public String getTransActionId()
	{
		return transActionId;
	}
	public void setTransActionId(String transActionId)
	{
		this.transActionId = transActionId;
	}
	public String getTransId()
	{
		return transId;
	}
	public void setTransId(String transId)
	{
		this.transId = transId;
	}
	public int getTransActionType()
	{
		return transActionType;
	}
	public void setTransActionType(int transActionType)
	{
		this.transActionType = transActionType;
	}
	public Timestamp getActionTime()
	{
		return actionTime;
	}
	public void setActionTime(Timestamp actionTime)
	{
		this.actionTime = actionTime;
	}
	public String getActioner()
	{
		return actioner;
	}
	public void setActioner(String actioner)
	{
		this.actioner = actioner;
	}
	public String getBoxActionId()
	{
		return boxActionId;
	}
	public void setBoxActionId(String boxActionId)
	{
		this.boxActionId = boxActionId;
	}
	public FeeProof getFeeProof()
	{
		return feeProof;
	}
	public void setFeeProof(FeeProof feeProof)
	{
		this.feeProof = feeProof;
	}
	public String getActionerName()
	{
		return actionerName;
	}
	public void setActionerName(String actionerName)
	{
		this.actionerName = actionerName;
	}
	public String getActionerMobileNum()
	{
		return actionerMobileNum;
	}
	public void setActionerMobileNum(String actionerMobileNum)
	{
		this.actionerMobileNum = actionerMobileNum;
	}
}
