package cn.com.xb.inter.domain.request;

import java.io.Serializable;
import java.util.Date;

public class CalculateCourierCostsWrapper extends BaseRequest implements
		Serializable {

	/**
	 * 计算快递费包裹信息封装
	 */
	private static final long serialVersionUID = 1L;

	private String provinceCode;// 收件人所在省级编码
	private String cityCode;// 收件人所在市级编码
	private String districtCountyCode;// 收件人所在区县编码
	private int expressSizeType;// 尺寸类型（1,超大/2,大/3,中/4,小/5,超小）
	private String clientUploadTime;// 上传提交时间 yyyy-MM-dd hh24:mm:ss

	public String getProvinceCode() {
		return provinceCode;
	}

	public void setProvinceCode(String provinceCode) {
		this.provinceCode = provinceCode;
	}

	public String getCityCode() {
		return cityCode;
	}

	public void setCityCode(String cityCode) {
		this.cityCode = cityCode;
	}

	public String getDistrictCountyCode() {
		return districtCountyCode;
	}

	public void setDistrictCountyCode(String districtCountyCode) {
		this.districtCountyCode = districtCountyCode;
	}

	public int getExpressSizeType() {
		return expressSizeType;
	}

	public void setExpressSizeType(int expressSizeType) {
		this.expressSizeType = expressSizeType;
	}

	public String getClientUploadTime() {
		return clientUploadTime;
	}

	public void setClientUploadTime(String clientUploadTime) {
		this.clientUploadTime = clientUploadTime;
	}

}
