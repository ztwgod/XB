package cn.com.xb.domain.base;

import java.io.Serializable;
import java.sql.Timestamp;

public class RoleModuleObject implements Serializable
{
	private static final long serialVersionUID = 1L;

	private String moduleId;		//模块ID
	private String moduleCode;		//模块代码
	private String parentMId;		//父级模块ID
	private String moduleName;		//模块名称
	private String moduleLink;		//模块链接
	private String moduleDesc;		//模块详情
	private int moduleLevel;		//模块级别
	private int showSeq;			//显示顺序
	private int moduleType;			//模块类型
	private Timestamp createTime;	//创建时间
	private String creator;			//创建用户ID
	private Timestamp enableTime;	//启用时间
	private int status;				//状态

	private int isAssignRole = 0;	//是否分配角色，0：未分配；1：已分配
	private int hasSubModule = 0;	//是否有子模块，0：无；1：有

	public String getModuleId()
	{
		return moduleId;
	}
	public void setModuleId(String moduleId)
	{
		this.moduleId = moduleId;
	}

	public String getModuleCode()
	{
		return moduleCode;
	}
	public void setModuleCode(String moduleCode)
	{
		this.moduleCode = moduleCode;
	}

	public String getParentMId()
	{
		return parentMId;
	}
	public void setParentMId(String parentMId)
	{
		this.parentMId = parentMId;
	}

	public String getModuleName()
	{
		return moduleName;
	}
	public void setModuleName(String moduleName)
	{
		this.moduleName = moduleName;
	}

	public String getModuleLink()
	{
		return moduleLink;
	}
	public void setModuleLink(String moduleLink)
	{
		this.moduleLink = moduleLink;
	}

	public String getModuleDesc()
	{
		return moduleDesc;
	}
	public void setModuleDesc(String moduleDesc)
	{
		this.moduleDesc = moduleDesc;
	}

	public int getModuleLevel()
	{
		return moduleLevel;
	}
	public void setModuleLevel(int moduleLevel)
	{
		this.moduleLevel = moduleLevel;
	}

	public int getShowSeq()
	{
		return showSeq;
	}
	public void setShowSeq(int showSeq)
	{
		this.showSeq = showSeq;
	}

	public int getModuleType()
	{
		return moduleType;
	}
	public void setModuleType(int moduleType)
	{
		this.moduleType = moduleType;
	}

	public Timestamp getCreateTime()
	{
		return createTime;
	}
	public void setCreateTime(Timestamp createTime)
	{
		this.createTime = createTime;
	}

	public String getCreator()
	{
		return creator;
	}
	public void setCreator(String creator)
	{
		this.creator = creator;
	}

	public Timestamp getEnableTime()
	{
		return enableTime;
	}
	public void setEnableTime(Timestamp enableTime)
	{
		this.enableTime = enableTime;
	}

	public int getStatus()
	{
		return status;
	}
	public void setStatus(int status)
	{
		this.status = status;
	}

	public int getIsAssignRole()
	{
		return isAssignRole;
	}
	public void setIsAssignRole(int isAssignRole)
	{
		this.isAssignRole = isAssignRole;
	}
	
	public int getHasSubModule()
	{
		return hasSubModule;
	}
	public void setHasSubModule(int hasSubModule)
	{
		this.hasSubModule = hasSubModule;
	}
}
